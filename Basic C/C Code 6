#include<stdio.h>
int main() {
    int num1, num2;
    int sum, sub, mul;
    float div;
    scanf("%d %d", &num1, &num2);
    sum = num1 + num2;
    sub = num1 - num2;
    mul = num1 * num2;
    div = (float)num1 / num2;
    printf("Addition = %d\n", sum);
    printf("Subtraction = %d\n", sub);
    printf("Muptiplication = %d\n", mul);
    printf("Division = %.2f\n", div);
    return 0;
}